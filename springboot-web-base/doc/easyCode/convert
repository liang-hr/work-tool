##导入宏定义
$!define

##设置表后缀（宏定义）
#setTableSuffix("Convert")

##保存文件（宏定义）
#save("/convert", "Convert.java")

##包路径（宏定义）
#setPackageSuffix("convert")

##定义服务名
#set($serviceName = $!tool.append($!tool.firstLowerCase($!tableInfo.name), "Convert"))

##定义实体对象名
#set($entityName = $!tool.firstLowerCase($!tableInfo.name))

##自动导入包（全局变量）
$!autoImport
import $!{tableInfo.savePackageName}.entity.$!tableInfo.name;
import $!{tableInfo.savePackageName}.qo.$!{tableInfo.name}Qo;
import $!{tableInfo.savePackageName}.vo.$!{tableInfo.name}Vo;
import org.springframework.web.bind.annotation.*;
import org.mapstruct.Mapper;

import javax.annotation.Resource;
import java.io.Serializable;
import java.util.List;

##表注释（宏定义）
#tableComment("表对象转换")
@Mapper(componentModel = "spring")
public interface $!{tableName}{
    /**
    * entity转换为vo
    */
    $!{tableInfo.name}Vo convert2Vo($!{tableInfo.name} $!{entityName});

    /**
    * qo转换为entity
    */
    $!{tableInfo.name} convertQo($!{tableInfo.name}Qo $!{entityName}Qo);

    /**
    * 列表：entity转换为vo
    */
    List<$!{tableInfo.name}Vo> convert2Vo(List<$!{tableInfo.name}> $!{entityName}s);

    /**
    * 列表：qo转换为entity
    */
    List<$!{tableInfo.name}> convertQo(List<$!{tableInfo.name}Qo> $!{entityName}Qos);

}